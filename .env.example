# Database Configuration
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/
MONGO_DB=travel_brain

# API Keys
OPENWEATHER_API_KEY=your_openweather_api_key_here
MAPBOX_TOKEN=your_mapbox_token_here

# Google OAuth
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Security
JWT_SECRET=your_jwt_secret_key_here
JWT_ALGORITHM=HS256
JWT_EXPIRES_IN=7d

# Application URLs
GOOGLE_CALLBACK_URL=https://yourdomain.com/api/auth/google/callback
FRONTEND_URL=https://yourdomain.com
BACKEND_API_URL=http://backend:3004
BUSINESS_RULES_API_URL=http://business-rules:3005

# Environment
NODE_ENV=production
APP_TIMEZONE=America/Guayaquil
CORS_ORIGINS=https://yourdomain.com,http://localhost:5173

# Face Recognition
FACE_DETECTION_BACKEND=retinaface
FACE_RECOGNITION_MODEL=Facenet512
SIMILARITY_THRESHOLD=0.6
FACE_DATA_PATH=/app/data/faces
TEMP_DATA_PATH=/app/data/temp
